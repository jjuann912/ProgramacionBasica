#include <stdio.h>
#include <stdlib.h>

int a, b, r;

int main(int argc, char *argv[]) {
	
	a=8;
	b=25;
	r=a+b;
	printf("El resultado de sumar %d y %d es %d\n", a, b , r);
	
	if(r == 40){
		printf("El resultado %d si es = 40", r);
	}
	else{
		if(r < 40){
			printf("el resultado %d si es < 40", r);
		}
		else{
		printf("El resultado no es = 40");
		}
	}
	return 0;
}

